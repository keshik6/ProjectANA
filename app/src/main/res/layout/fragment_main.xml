<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="sutdcreations.projectana.MainActivity"
    android:background="@drawable/background">





    <LinearLayout
        android:id="@+id/HoldMyButtons"
        android:layout_width="match_parent"
        android:layout_height="379dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="76dp"
        android:layout_centerVertical="true">

        <Button
            android:id="@+id/testProfile"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginTop="13dp"
            android:backgroundTint="#60C3CE"
            android:fontFamily="@font/moon_bold"
            android:text="My profile"
            android:textColor="#000000"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="99dp" />

        <Button
            android:id="@+id/testCourse"
            android:layout_width="250dp"
            android:layout_height="80dp"
            android:layout_marginTop="12dp"
            android:backgroundTint="#6F6FEE"
            android:fontFamily="@font/moon_bold"
            android:text="See my courses"
            android:textColor="#000000"
            android:textSize="20sp"
            android:visibility="gone"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="43dp" />

        <Button
            android:id="@+id/testFeedback"
            android:layout_width="250dp"
            android:layout_height="80dp"
            android:layout_marginTop="13dp"
            android:backgroundTint="#AB80E9"
            android:fontFamily="@font/moon_bold"
            android:text="Test Feedback"
            android:textColor="#000000"
            android:textSize="20sp"
            android:visibility="gone" />

        <Button
            android:id="@+id/testSnoop"
            android:layout_width="250dp"
            android:layout_height="80dp"
            android:layout_marginTop="11dp"
            android:backgroundTint="#CA6C94"
            android:fontFamily="@font/moon_bold"
            android:text="(Prof) Snoop on my students"
            android:textColor="#000000"
            android:visibility="gone" />

    </LinearLayout>

</LinearLayout>